{
  "[markdown]": {
    "editor.fontFamily": "'iA Writer Quattro V', monospace",
    "editor.fontSize": 16,
    "editor.lineNumbers": "off",
    "editor.renderWhitespace": "none",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80
  },
  "breadcrumbs.enabled": false,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "editor.bracketPairColorization.enabled": false,
  "editor.cursorBlinking": "smooth",
  "editor.cursorStyle": "line",
  "editor.fontFamily": "'MD IO Trial', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": false,
  "editor.lineHeight": 2.0,
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 100,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "window.autoDetectColorScheme": true,
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "never",
  "window.nativeTabs": true,
  "workbench.activityBar.location": "hidden",
  "workbench.activityBar.orientation": "vertical",
  "workbench.colorTheme": "Ayu Light Bordered",
  "workbench.editor.tabSizing": "shrink",
  "workbench.iconTheme": "vs-minimal",
  "workbench.layoutControl.enabled": false,
  "workbench.panel.defaultLocation": "right",
  "workbench.preferredDarkColorTheme": "Ayu Dark Bordered",
  "workbench.preferredLightColorTheme": "Ayu Light Bordered",
  "workbench.settings.editor": "json",
  "workbench.sideBar.location": "right",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always"
}
