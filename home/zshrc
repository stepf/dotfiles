#!/usr/bin/env zsh

export ZSH="$HOME/.oh-my-zsh"                     # oh my zsh path

# SETUP
. `brew --prefix`/etc/profile.d/z.sh           # z
source $HOME/.zsh_oh_my_zsh                    # oh my zsh settings
source $HOME/.oh-my-zsh/oh-my-zsh.sh           # oh my zsh setup
source $HOME/.zsh_aliases                      # aliases
source $HOME/.zsh_functions                    # functions

# FUN
apple                                          # ascii retro apple logo
